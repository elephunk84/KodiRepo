<addons>
<addon id="plugin.program.iainstool" name="Iain's Update Tool" version="1.0.2" provider-name="iaincstott.co.uk">
  <requires>
      <import addon="xbmc.python" version="2.19.0"/>
      <import addon="script.module.httplib2" version="0.8.0" />
      <import addon="script.module.oauth2client" version="1.4.7" />
      <import addon="script.module.uritemplate" version="0.6" />
      <import addon="script.module.yaml" version="3.11"/>
      <import addon="script.module.googleapi" version="1.4.0" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>program executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Iain's Update Tool</summary>
    <description lang="en">Easy Install With Iain's Update Tool</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="script.tvguide.fullscreen" name="TV Guide Fullscreen"
version="0.0.350"
provider-name="primaeval">
    <requires>
        <import addon="xbmc.python" version="2.19.0"/>
        <import addon="script.module.simplejson" version="3.3.0"/>
        <import addon="script.module.requests" version="2.9.1" />
        <import addon="script.module.dateutil" version="2.4.2" />
        <import addon="script.module.pil" />
        <import addon="script.module.beautifulsoup4" />
    </requires>
    <extension point="xbmc.python.script" library="addon.py">
            <provides>executable</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="login"/>
    <extension point="xbmc.addon.metadata">
        <description lang="en">Possibly the most powerful TV Guide in the world. ;) Thanks for the major contributions from: iainmacleod, im85288, RayW1986, bluezed, andy, Zomboided, tycoo, kjb86.</description>
        <disclaimer lang="en">Based on the original TV Guide addon by twinther, FTV Guide and Midraal's fork.</disclaimer>
        <language></language>
        <platform>all</platform>
        <license>GPLv2</license>
        <forum>http://forum.kodi.tv/showthread.php?tid=282157</forum>
        <source>https://github.com/primaeval/script.tvguide.fullscreen</source>
        <website>https://github.com/primaeval/script.tvguide.fullscreen</website>
        <email>primaeval</email>
    </extension>
</addon>

